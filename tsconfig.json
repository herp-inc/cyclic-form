{
  "compilerOptions": {
    "declaration": true,
    "keyofStringsOnly": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "lib",
    "strict": true,
    "strictFunctionTypes": false,
    "target": "ESNext"
  },
  "exclude": ["lib", "node_modules"]
}
